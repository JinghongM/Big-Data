SQL QUERY

sqoop eval --connect jdbc:mysql://localhost/retail_db \
--username root \
--password hadoop \
--query "SELECT * FROM orders LIMIT 10"

sqoop eval --connect jdbc:mysql://localhost/retail_db \
--username root \
--password hadoop \
--query \
"INSERT INTO orders VALUES (100000,"2017-10-31 00:00:00.0" 100000 'DUMMY')"